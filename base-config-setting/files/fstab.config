config 'global'
	option	anon_swap	'0'
	option	anon_mount	'0'
	option	auto_swap	'1'
	option	auto_mount	'1'
	option	delay_root	'5'
	option	check_fs	'0'

config 'mount' 'overlay'
	option	target	'/overlay'
	option	uuid	'f3178596-4427-2d3b-35c7-648b65e20d5e' # echo -n  X-WRT-overlay | md5sum
	option	fstype	'ext4'
	option	options	'rw,noatime'
	option	enabled	'1'

config 'mount' 'data'
	option	target	'/data'
	option	uuid	'db0482e8-04bd-dfbb-3935-fbc9d9a9cfb9' # echo -n  X-WRT-data | md5sum
	option	fstype	'ext4'
	option	options	'rw,noatime'
	option	enabled	'1'
